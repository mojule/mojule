[
  {
    "id": "document",
    "title": "HTML Document",
    "type": "object",
    "required": [
      "title"
    ],
    "meta": {
      "componentName": "document",
      "transform": false
    },
    "default": {
      "title": "Home",
      "styles": [
        {
          "text": "body{ font-family: sans-serif; }"
        }
      ]
    }
  },
  [
    {
      "title": "Document Title",
      "type": "string",
      "propertyName": "title",
      "meta": {
        "componentName": "non-empty-string",
        "transform": false
      },
      "pattern": "^(?!\\s*$).+"
    }
  ],
  [
    {
      "title": "Styles",
      "type": "array",
      "propertyName": "styles"
    },
    [
      {
        "title": "Style",
        "type": "any",
        "arrayItem": true,
        "meta": {
          "componentName": "style",
          "transform": true
        }
      },
      [
        {
          "title": "Resource as Text",
          "type": "object",
          "oneOf": true,
          "meta": {
            "componentName": "source-text",
            "transform": false
          },
          "required": [
            "text"
          ]
        },
        [
          {
            "title": "Text Content",
            "type": "string",
            "propertyName": "text"
          }
        ]
      ],
      [
        {
          "title": "Resource as URI",
          "type": "object",
          "oneOf": true,
          "meta": {
            "componentName": "source-uri",
            "transform": false
          },
          "required": [
            "uri"
          ]
        },
        [
          {
            "title": "URI",
            "type": "string",
            "propertyName": "uri"
          }
        ]
      ]
    ]
  ],
  [
    {
      "title": "Scripts",
      "type": "array",
      "propertyName": "scripts"
    },
    [
      {
        "title": "Script",
        "type": "any",
        "arrayItem": true,
        "meta": {
          "componentName": "script",
          "transform": true
        }
      },
      [
        {
          "type": "boolean",
          "propertyName": "isHeadScript"
        }
      ]
    ]
  ],
  [
    {
      "title": "Component Children",
      "type": "object",
      "propertyName": "_children"
    },
    [
      {
        "id": "header",
        "title": "Page Header",
        "type": "object",
        "required": [
          "links",
          "secondary-links"
        ],
        "meta": {
          "componentName": "header",
          "transform": false
        },
        "default": {
          "links": [
            {
              "title": "Home",
              "uri": "/"
            },
            {
              "title": "About",
              "uri": "/about"
            }
          ]
        },
        "propertyName": "0"
      },
      [
        {
          "title": "Primary Navigation Links",
          "type": "array",
          "propertyName": "links"
        },
        [
          {
            "title": "Link",
            "type": "object",
            "arrayItem": true,
            "meta": {
              "componentName": "link",
              "transform": true
            },
            "required": [
              "title",
              "uri"
            ]
          },
          [
            {
              "title": "Link Text",
              "type": "string",
              "propertyName": "title",
              "meta": {
                "componentName": "non-empty-string",
                "transform": false
              },
              "pattern": "^(?!\\s*$).+"
            }
          ],
          [
            {
              "title": "URI",
              "type": "string",
              "propertyName": "uri"
            }
          ]
        ]
      ],
      [
        {
          "title": "Secondary Navigation Links",
          "type": "array",
          "propertyName": "secondary-links"
        },
        [
          {
            "title": "Link",
            "type": "object",
            "arrayItem": true,
            "meta": {
              "componentName": "link",
              "transform": true
            },
            "required": [
              "title",
              "uri"
            ]
          },
          [
            {
              "title": "Link Text",
              "type": "string",
              "propertyName": "title",
              "meta": {
                "componentName": "non-empty-string",
                "transform": false
              },
              "pattern": "^(?!\\s*$).+"
            }
          ],
          [
            {
              "title": "URI",
              "type": "string",
              "propertyName": "uri"
            }
          ]
        ]
      ]
    ],
    [
      {
        "type": "object",
        "meta": {
          "componentName": "main",
          "transform": false
        },
        "title": "main",
        "default": {},
        "propertyName": "1"
      },
      [
        {
          "title": "Component Children",
          "type": "object",
          "propertyName": "_children"
