              "transform": false
            },
            "title": "nutrition-comparison",
            "default": {},
            "propertyName": "1"
          }
        ]
      ]
    ],
    [
      {
        "type": "object",
        "meta": {
          "componentName": "footer",
          "transform": false
        },
        "title": "footer",
        "default": {},
        "propertyName": "2"
      }
    ]
  ]
]
